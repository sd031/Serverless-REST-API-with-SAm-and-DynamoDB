# Example SAM configuration file
# Copy this to samconfig.toml and customize for your environment

version = 0.1

[default]
[default.deploy]
[default.deploy.parameters]
stack_name = "serverless-users-api"
s3_prefix = "serverless-users-api"
region = "us-west-2"
capabilities = "CAPABILITY_IAM"
confirm_changeset = true
resolve_s3 = true
fail_on_empty_changeset = false

[default.build]
[default.build.parameters]
cached = true
parallel = true
